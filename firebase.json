{
  "hosting": {
    "public": "dist/pwa",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "!/__",
        "destination": "/index.html"
     },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
